{ 
  "manifest_version": 2, 
  "name": "concept map", 
  "version": "0.1.0",
  "description": "An extension to develop a concept-map for the core topics on the active tab",
  "permissions": ["<all_urls>", "contextMenus"], 
  "browser_action": { 
    "default_popup": "index.html" 
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon24.png"
  }
}